{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MasterConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=master;Trusted_Connection=True;",
    "DefaultConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=your-database-name;Trusted_Connection=True;"
  },
  "JwtSettings": {
    "SecretKey": "your-secret-key",
    "Issuer": "https://localhost:7003",
    "Audience": "https://localhost:7003",
    "ExpiresInDays": 5
  },
  "AllowedHosts": "*"
}
